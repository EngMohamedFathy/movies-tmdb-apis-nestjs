										"exec": [
											"pm.test(\"Token Set\", function () {\r",
											"    var jsonData = pm.response.json();\r",
											"\r",
											"    // Check if the 'token' property exists in the response\r",
											"    if (jsonData && jsonData.data.tokens) {\r",
											"        // Assign the token value to the 'token_member' variable\r",
											"        pm.environment.set('nest_adminToken', jsonData.data.tokens.access);\r",
											"        pm.expect(jsonData.data.token).to.exist;\r",
											"    } else {\r",
											"        // Assert a failure condition if the token is not found\r",
											"        pm.expect.fail(\"Token not found in the response\");\r",
											"    }\r",
											"});\r",
